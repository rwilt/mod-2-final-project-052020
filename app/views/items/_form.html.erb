<%= form_for @item do |f| %>
     <%= f.label :name %>
     <%= f.text_field :name %>

    <%= f.label :price %>
    <%= f.number_field :price %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :photo %>
    <%= f.text_field :photo %>

    <%= f.submit %>
  
<% end %>