<%= form_for @seller do |f| %>
    <p>
        <%=f.label :name%>
        <%=f.text_field :name%>
    </p>
    <p>
        <%=f.label :bio%>
        <%=f.text_area :bio%>
    </p>
    <!-- how can users upload photos -->
    <p>
        <%=f.label :photo%>
        <%=f.text_field :photo%>
    </p>
    <p>
        <%=f.label :phone%>
        <%=f.telephone_field :phone%>
    </p>
    <p>
        <%=f.label :email%>
        <%=f.text_field :email%>
    </p>
    <p>
        <%=f.label "Zip Code"%>
        <%=f.number_field :zip%>
    </p>
    <p><%=f.submit "Let's Get Started!"%></p>
<% end %>